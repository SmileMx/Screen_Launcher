<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal" >
	<ImageView 
        android:id="@+id/photos_show_ImageView01"
        android:layout_width="@dimen/photos_show_big_width"
        android:layout_height="@dimen/photos_show_big_height"
        android:layout_marginTop="@dimen/photos_show_top"
        android:layout_marginRight="@dimen/photos_show_gap"
        android:background="@drawable/regist_acount_line"
        android:scaleType="centerCrop"/>
    <ImageView 
        android:id="@+id/photos_show_ImageView02"
        android:layout_width="@dimen/photos_show_small_width"
        android:layout_height="@dimen/photos_show_small_height"
        android:layout_marginTop="@dimen/photos_show_top"
        android:scaleType="centerCrop"
        android:background="@drawable/regist_acount_line"
        android:layout_toRightOf="@+id/photos_show_ImageView01"/>
    <ImageView 
        android:id="@+id/photos_show_ImageView03"
        android:layout_width="@dimen/photos_show_small_width"
        android:layout_height="@dimen/photos_show_small_height"
        android:layout_below="@+id/photos_show_ImageView01"
        android:layout_marginTop="@dimen/photos_show_gap"
        android:scaleType="centerCrop"
        android:background="@drawable/regist_acount_line"
        android:layout_toRightOf="@+id/photos_show_ImageView01"/>
    
    <TextView 
        android:id="@+id/photos_show_randomcolor_TextView"
        android:layout_width="@dimen/photos_show_big_width"
        android:layout_height="@dimen/photos_show_small_height"
        android:layout_marginTop="@dimen/photos_show_gap"
        android:layout_marginRight="@dimen/photos_show_gap"
        android:layout_below="@+id/photos_show_ImageView01"     
        android:background="#ecad00"
        android:textColor="#ffffff"
        android:textSize="@dimen/photos_show_randomcolor_TextView_size"
        android:gravity="center"/>
    <ImageView 
        android:id="@+id/photos_show_ImageView04"
        android:layout_width="@dimen/photos_show_small_width"
        android:layout_height="@dimen/photos_show_small_height"
        android:layout_marginTop="@dimen/photos_show_gap"
        android:scaleType="centerCrop"
        android:background="@drawable/regist_acount_line"
        android:layout_below="@+id/photos_show_ImageView02"
        android:layout_toRightOf="@+id/photos_show_randomcolor_TextView"/>	
	
	<ImageView 
        android:id="@+id/photos_show_ImageView05"
        android:layout_width="@dimen/photos_show_small_width"
        android:layout_height="@dimen/photos_show_small_height"
        android:layout_marginTop="@dimen/photos_show_gap"      
        android:scaleType="centerCrop"
        android:background="@drawable/regist_acount_line"
        android:layout_below="@+id/photos_show_randomcolor_TextView"/>
	<ImageView 
        android:id="@+id/photos_show_ImageView06"
        android:layout_width="@dimen/photos_show_small_width"
        android:layout_height="@dimen/photos_show_small_height"
        android:layout_marginTop="@dimen/photos_show_gap"      
        android:background="@drawable/regist_acount_line"
        android:scaleType="centerCrop"
        android:layout_below="@+id/photos_show_ImageView05"/>
	<ImageView 
        android:id="@+id/photos_show_ImageView07"
        android:layout_width="@dimen/photos_show_big_width"
        android:layout_height="@dimen/photos_show_big_height"
        android:layout_below="@+id/photos_show_randomcolor_TextView"
        android:layout_toRightOf="@+id/photos_show_ImageView05"
        android:layout_marginTop="@dimen/photos_show_gap"
        android:layout_marginLeft="@dimen/photos_show_gap"
        android:background="@drawable/regist_acount_line"
        android:scaleType="centerCrop"/>
	<TextView
        android:id="@+id/photos_show_pagesize"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignRight="@+id/photos_show_ImageView07"
        android:layout_below="@+id/photos_show_ImageView07"
        android:layout_marginTop="@dimen/photos_show_pageshow_top"
        android:text="7"
        android:textColor="@color/white"
        android:textSize="@dimen/photos_show_pageshow_textsize" />

    <TextView
        android:id="@+id/photos_show_tmp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/photos_show_pagesize"
        android:layout_toLeftOf="@+id/photos_show_pagesize"
        android:text="/"
        android:textColor="@color/white"
        android:textSize="@dimen/photos_show_pageshow_textsize" />

    <TextView
        android:id="@+id/photos_show_page_TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/photos_show_tmp"
        android:layout_toLeftOf="@+id/photos_show_tmp"
        android:text="1"
        android:textColor="@color/white"
        android:textSize="@dimen/photos_show_pageshow_textsize" />
	
</RelativeLayout>